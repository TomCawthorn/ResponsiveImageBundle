{
    "name": "irishdan/responsive-image-bundle",
    "description": "A bundle for creating styled and responsive images for symfony projects",
    "license": "MIT",
    "homepage": "http://danbyrne.me/projects/responsive-image-bundle",
    "authors": [
        {
            "name": "Daniel Byrne",
            "email": "danielbyrne@outlook.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "doctrine/doctrine-bundle": "^1.6",
        "intervention/image": "^2.3",
        "oneup/flysystem-bundle": "^1.13",
        "symfony/framework-bundle": "^3.2",
        "symfony/twig-bundle": "^3.2",
        "symfony/validator": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": {
            "IrishDan\\ResponsiveImageBundle\\": ""
        }
    },
    "suggest": {
        "aws/aws-sdk-php": "^3.18"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
