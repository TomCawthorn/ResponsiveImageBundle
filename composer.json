{
    "name": "tomcawthorn/responsive-image-bundle",
    "description": "A bundle for creating styled and responsive images for symfony projects",
    "license": "MIT",
    "homepage": "http://danbyrne.me/projects/responsive-image-bundle",
    "authors": [
        {
            "name": "Daniel Byrne",
            "email": "danielbyrne@outlook.com"
        }
    ],
    "require": {
        "php": ">=7.2.9",
        "doctrine/doctrine-bundle": "^1.10",
        "doctrine/orm": "^2.6",
        "intervention/image": "^2.4.2",
        "oneup/flysystem-bundle": "^3.0.3",
        "symfony/monolog-bundle": "^3.3.1",
        "symfony/property-access": "^4.*",
        "symfony/twig-bundle": "^4.*",
        "symfony/validator": "^4.*",
        "symfony/yaml": "^4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "symfony/maker-bundle": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "IrishDan\\ResponsiveImageBundle\\": ""
        }
    },
    "suggest": {
        "aws/aws-sdk-php": "^3.8"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
